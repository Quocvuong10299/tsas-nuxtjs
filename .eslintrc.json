{
  "root": true,
  "parserOptions": {
    "parser": "babel-eslint",
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "process": true
  },
  "extends": ["plugin:nuxt/recommended", "eslint:recommended", "vue"],
  "plugins": ["eslint-plugin", "prettier"],
  "rules": {
    "semi": ["error", "always"]
  }
}
