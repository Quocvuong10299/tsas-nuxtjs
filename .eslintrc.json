{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "process": true
  },
  "extends": ["plugin:prettier/recommended", "prettier", "vue", "@nuxtjs"],
  "parserOptions": {
    "ecmaVersion": 9,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": ["eslint-plugin", "prettier"],
  "rules": {}
}
